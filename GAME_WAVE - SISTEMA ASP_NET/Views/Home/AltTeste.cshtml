<!DOCTYPE HTML>
<html lang="pt-br">
@model GAME_WAVE___SISTEMA_ASP_NET.Models.Classe_Teste
@{
    ViewBag.Title = "Alteração do Test Drive";
}
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/Content/background_imagens.css" />


</head>
<body>
    <br />
    <br />
    <legend><b>Alteração do Test Drive</b></legend>
    <div class="container" id="">
        @using (Html.BeginForm("AltTeste", "Home", FormMethod.Post))
        {
            @Html.ValidationSummary(true)
            <fildset>
                <div class="editor-label" style="font-size: 30px;">
                    @Html.LabelFor(m => m.Age_DtRetirada, htmlAttributes: new { @class = "" })
                </div>

                <div class="editor-field">
                    @Html.EditorFor(m => m.Age_DtRetirada, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(m => m.Age_DtRetirada, "", new { @class = "" })
                </div>

                <div class="editor-label" style="font-size: 30px; margin-left: 400px; margin-top: -73px;">
                    @Html.LabelFor(m => m.Age_Desc, htmlAttributes: new { @class = "" })
                </div>

                <div class="editor-field" style="margin-left: 400px; ">
                    @Html.EditorFor(m => m.Age_Desc, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(m => m.Age_Desc, "", new { @class = "" })
                </div>

                <div class="editor-label" style="font-size: 30px; margin-top: 50px;">
                    @Html.LabelFor(m => m.Fk_Cliente_Cli_cpf, htmlAttributes: new { @class = "" })
                </div>

                <div class="editor-field">
                    @Html.EditorFor(m => m.Fk_Cliente_Cli_cpf, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(m => m.Fk_Cliente_Cli_cpf, "", new { @class = "" })
                </div>

                <div class="editor-label" style="font-size: 30px; margin-left: 400px; margin-top: -73px;">
                    @Html.LabelFor(m => m.Fk_Produto_Cod_Prod, htmlAttributes: new { @class = "" })
                </div>

                <div class="editor-field" style="margin-left: 400px; ">
                    @Html.EditorFor(m => m.Fk_Produto_Cod_Prod, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(m => m.Fk_Produto_Cod_Prod, "", new { @class = "" })
                </div>

                <div class="editor-label" style="font-size: 30px; margin-top: 50px;">
                    @Html.LabelFor(m => m.Agen_Cod, htmlAttributes: new { @class = "" })
                </div>

                <div class="editor-field">
                    @Html.EditorFor(m => m.Agen_Cod, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(m => m.Agen_Cod, "", new { @class = "" })
                </div>

                <div>
                    <br /> <input type="submit" value="Alterar" name="btnenviar" />
                </div>
            </fildset>
            @section Scripts{
                @Scripts.Render("~/bundles/jqueryval");
            }

        }
    </div>

