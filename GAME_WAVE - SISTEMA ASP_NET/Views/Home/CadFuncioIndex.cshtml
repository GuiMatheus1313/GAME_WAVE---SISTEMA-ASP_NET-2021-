                <div class="editor-field" style="margin-left: 500px;">
                    @Html.EditorFor(m => m.Func_num_end)
                    @Html.ValidationMessageFor(m => m.Func_num_end)
                </div>

                <div class="editor-label" style="font-size: 30px;">
                    @Html.LabelFor(m => m.Func_senha)
                </div>

                <div class="editor-field">
                    @Html.PasswordFor(m => m.Func_senha)
                    @Html.ValidationMessageFor(m => m.Func_senha)
                </div>

                <div>
                    <br /> <input type="submit" value="Cadastrar" name="btnenviar" />
                </div>
            </fildset>
            @section Scripts{
                @Scripts.Render("~/bundles/jqueryval");
            }
        }
    </div>
